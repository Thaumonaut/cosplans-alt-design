{
  "name": "cosplay-tracker-sveltekit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test": "vitest run && playwright test",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:cleanup": "bun run tests/utils/cleanup.ts",
    "test:verify": "bun run scripts/verify-test-environment.ts",
    "build:production": "NODE_ENV=production vite build",
    "build:stats": "vite build --mode production && npx vite-bundle-analyzer dist",
    "deploy:cf": "wrangler deploy .svelte-kit/cloudflare/_worker.js"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@melloware/coloris": "^0.25.0",
    "@neodrag/svelte": "^2.3.3",
    "@number-flow/svelte": "^0.3.9",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/svelte-virtual": "^3.13.12",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color2k": "^2.0.3",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.3",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.19.0",
    "formsnap": "^2.0.1",
    "fuse.js": "^7.1.0",
    "lucide-svelte": "^0.454.0",
    "supabase": "^2.54.11",
    "svelte-dnd-action": "^0.9.67",
    "svelte-multiselect": "^11.2.4",
    "svelte-sound": "^0.6.0",
    "svelte-teleport": "^0.1.1",
    "sveltekit-superforms": "^2.28.1",
    "tailwind-merge": "^2.5.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-cloudflare": "^4.6.0",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^22",
    "@vitest/ui": "4.0.3",
    "autoprefixer": "^10.4.20",
    "jsdom": "^27.0.1",
    "postcss": "^8.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^4.0.3"
  },
}
